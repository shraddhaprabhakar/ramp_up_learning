}

//CONVERTING STRING TO INTEGER
use std::io
fn main() {
  let mut input = String::new();
  io::stdin().read_line(&mut input).expect("could not read line");
  let int_input: i64 = input.trim().parse().unwrap(); //
  println!("{}", input);
}
